    "@mui/x-data-grid": "^7.18.0",
    "@tanstack/react-query": "^5.56.2",
    "d3": "^7.9.0",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "simplebar-react": "^3.2.6",
    "swiper": "^11.1.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@iconify/react": "^5.0.2",
    "@types/d3": "^7.4.3",
